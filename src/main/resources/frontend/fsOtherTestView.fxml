<VBox fillWidth="true" spacing="10" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10"
      AnchorPane.topAnchor="10" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.controller.FSOtherTestController">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    <children>
        <VBox alignment="CENTER_LEFT" spacing="10">
            <Label text="测试进度:">
                <font>
                    <Font size="18.0" />
                </font>
            </Label>
            <TextArea fx:id="currentStepTextArea" prefHeight="200.0" prefWidth="200.0" />
        </VBox>

        <Label text="测试结果:">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <TableView fx:id="fioResultsTableView" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="metricsColumn" text="性能指标"/>
                <TableColumn fx:id="resultColumn" text="结果"/>
                <!-- Additional columns as needed -->
            </columns>
        </TableView>

